echo "Compiling: main.c to hiddenstar"
gcc -std=c99 -g -c main.c gui.c menu.c intro.c planets.c starmap.c combat.c comms.c factions.c util/vector.c -lSDL2_image `sdl2-config --cflags`
gcc -std=c99 -o hiddenstar main.o gui.o menu.o intro.o planets.o starmap.o combat.o comms.o factions.o vector.o -lSDL2_image `sdl2-config --libs` -lm
