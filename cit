echo "Compiling: main.c to hiddenstar"
gcc -std=c99 -c main.c gui.c intro.c planets.c combat.c comms.c travel.c util/vector.c -O2 -lSDL2_image `sdl2-config --cflags`
gcc -std=c99 -o hiddenstar main.o gui.o intro.o planets.o combat.o comms.o travel.o vector.o -O2 -lSDL2_image `sdl2-config --libs` -lm
